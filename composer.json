{
	"name": "rkr/dispatcher",
	"description": "A simple dispatcher with dependency injection support",
	"homepage": "https://github.com/rkrx/php-dispatcher",
	"license": "MIT",
	"type": "library",
	"minimum-stability": "stable",
	"authors": [{
		"name": "Ron Kirschler",
		"email": "ron.kirschler@gmail.com"
	}],
	"require": {
		"php": ">= 5.3.0"
	},
	"require-dev": {
		"phpunit/phpunit": "3.7.*"
	},
	"autoload": {
		"psr-4": {
			"Kir\\Dispatching\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Kir\\Dispatching\\": ["src/", "tests/"]
		}
	}
}